{
  "compilerOptions": {
    "paths": {
      "src/*": ["./src/*"],
      "lib/*": ["./src/lib/*"],
      "assets/*": ["./src/assets/*"],
      "app/*": ["./src/app/*"],
      "components/*": ["./src/app/components/*"],
      "auth/*": ["./src/app/auth/*"],
      "utils": ["./src/utils/*"],
      "api/*": ["./src/api/*"],
      "typeRoots": ["./src/types"],
      "context": ["./src/context/*"],
      "hooks": ["./src/hooks/*"]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
